{
    "name": "cakemanager/cakephp-api",
    "description": "Api plugin for CakePHP 3.x",
    "type": "cakephp-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Bob Mulder",
            "email": "bobmulder@outlook.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "cakephp/cakephp": "~3.0",
        "cakemanager/cakephp-utils": "1.0.*@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Api\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Api\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
